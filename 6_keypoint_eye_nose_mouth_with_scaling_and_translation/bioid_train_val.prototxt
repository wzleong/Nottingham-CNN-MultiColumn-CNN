###########################################################
## Conv1+pool1 1
###########################################################
layer {
    	name: "conv1"
    	type: "Convolution"
	bottom: "data"
    	top: "conv1"
    	convolution_param {
		num_output: 32
		kernel_size: 13
		stride: 2
		pad: 0
		weight_filler {
      			type: "xavier"
		}
		bias_filler {
			type: "constant"
		}
	}	
 	# learning rate and decay multipliers for the filters
 	param { 
		lr_mult: 1
		decay_mult: 1
	}
  	# learning rate and decay multipliers for the biases
  	param { 
		lr_mult: 2
		decay_mult: 0 
	}
}
layer {
	name: "relu1"
	type: "ReLU"
	bottom: "conv1"
	top: "conv1"
}
layer {
	name: "pool1"
	type: "Pooling"
	bottom: "conv1"
	top: "pool1"
	pooling_param {
		pool: MAX
		kernel_size: 2
		stride: 2
	} 
}

layer {
    	name: "conv2_1"
    	type: "Convolution"
	bottom: "pool1"
    	top: "conv2_1"
    	convolution_param {
		num_output: 32
		kernel_size: 5
		stride: 1
		pad: 0
		weight_filler {
      			type: "xavier"
		}
		bias_filler {
			type: "constant"
		}
	}	
 	# learning rate and decay multipliers for the filters
 	param { 
		lr_mult: 1
		decay_mult: 1
	}
  	# learning rate and decay multipliers for the biases
  	param { 
		lr_mult: 2
		decay_mult: 0 
	}
}
layer {
	name: "relu2_1"
	type: "ReLU"
	bottom: "conv2_1"
	top: "conv2_1"
}
layer {
    	name: "conv2_2"
    	type: "Convolution"
	bottom: "conv2_1"
    	top: "conv2_2"
    	convolution_param {
		num_output: 64
		kernel_size: 5
		stride: 1
		pad: 0
		weight_filler {
      			type: "xavier"
		}
		bias_filler {
			type: "constant"
		}
	}	
 	# learning rate and decay multipliers for the filters
