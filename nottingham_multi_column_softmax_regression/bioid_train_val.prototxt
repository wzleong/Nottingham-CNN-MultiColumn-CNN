      			type: "xavier"
    		}
    		bias_filler {
      			type: "constant"
    		}
	}
 	# learning rate and decay multipliers for the filters
 	param { 
		lr_mult: 1 
		decay_mult: 0
	}
  	# learning rate and decay multipliers for the biases (usually double for better convergence)
  	param { 
		lr_mult: 2
		decay_mult: 0 
	}
}
layer {
	name: "relu_fc2_right_confidence"
	type: "ReLU"
	bottom: "fc2_right_confidence"
	top: "fc2_right_confidence"
}
layer {
	name: "drop2_right_confidence"
  	type: "Dropout"
  	bottom: "fc2_right_confidence"
	top: "fc2_right_confidence"
	dropout_param {
 	dropout_ratio: 0.5
  	}
}
layer {
	name: "fc3_right_confidence"
	type: "Convolution"
	bottom: "fc2_right_confidence"
	top: "fc3_right_confidence"
	convolution_param {
