		num_output: 4
		kernel_size: 3
		stride: 2
		pad: 1
		weight_filler {
			type: "xavier"
		}
		bias_filler {
			type: "constant"
		}
	}	
 	# learning rate and decay multipliers for the filters
 	param { 
		lr_mult: 1
