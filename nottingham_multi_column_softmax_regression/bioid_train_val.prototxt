  	param { 
		lr_mult: 2
		decay_mult: 0 
	}
}
layer {
	name: "loss_right_confidence"
	type: "SoftmaxWithLoss"
	bottom: "fc3_right_confidence"
	bottom: "visibility_right"
	top: "loss_right_confidence"
