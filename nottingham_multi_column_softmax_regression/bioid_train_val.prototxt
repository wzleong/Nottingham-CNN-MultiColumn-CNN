   		num_output: 2
		kernel_size: 1
		stride: 1
		pad: 0
 		weight_filler {
      			type: "xavier"
    		}
    		bias_filler {
      			type: "constant"
    		}
	}
 	# learning rate and decay multipliers for the filters
